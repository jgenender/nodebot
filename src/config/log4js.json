{
  "appenders": {
    "fileAppender": {
      "type": "dateFile",
      "filename": "./logs/nodebot.log",
      "maxLogSize": 10485760,
      "backups": 7,
      "compress": true
    },
    "console": {
      "type": "console"
    }
  },
  "categories": {
    "default": {
      "appenders": [
        "fileAppender",
        "console"
      ],
      "level": "debug"
    }
  }
}